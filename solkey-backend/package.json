{
  "name": "solkey-backend",
  "version": "1.0.0",
  "description": "SolKey Backend Service",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "cross-env NODE_ENV=test dotenv -e .env.test -- jest",
    "test:watch": "cross-env NODE_ENV=test dotenv -e .env.test -- jest --watch",
    "test:integration": "cross-env NODE_ENV=test dotenv -e .env.test -- jest -c jest.integration.config.js",
    "lint": "eslint src/",
    "format": "prettier --write 'src/**/*.js'"
  },
  "dependencies": {
    "@solana/web3.js": "^1.98.2",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.2",
    "bs58": "^5.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.21.2",
    "express-rate-limit": "^7.5.0",
    "helmet": "^7.2.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mongodb": "^6.16.0",
    "mongoose": "^8.14.2",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "tweetnacl": "^1.0.3"
  },
  "devDependencies": {
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.0.1",
    "prettier": "^3.1.0",
    "supertest": "^6.3.4"
